<?xml version="1.0" encoding="UTF-8"?>
<executable>
  <category>Example</category>
  <title>Example 3 - Compare Inputs</title>
  <description>Compare an item, image, and file.</description>
  <version>0.2.0</version>
  <license>Apache 2.0</license>
  <contributor>David Manthey (Kitware)</contributor>
  <parameters>
    <label>Inputs</label>
    <description>Inputs to compare</description>
    <file>
      <name>file1</name>
      <longflag>file1</longflag>
      <description>An input file</description>
      <label>Input file</label>
      <channel>input</channel>
    </file>
    <image>
      <name>image1</name>
      <longflag>image1</longflag>
      <description>An input image</description>
      <label>Input image</label>
      <channel>input</channel>
    </image>
    <item>
      <name>item1</name>
      <longflag>item1</longflag>
      <description>An input item</description>
      <label>Input item</label>
      <channel>input</channel>
    </item>
  </parameters>
</executable>

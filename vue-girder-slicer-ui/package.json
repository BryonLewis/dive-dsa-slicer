{
  "name": "@bryonlewis/vue-girder-slicer-ui",
  "version": "0.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/VueGirderSlicerCLIUI.umd.js",
  "module": "./dist/VueGirderSlicerCLIUI.es.js",
  "exports": {
    ".": {
      "import": "./dist/VueGirderSlicerCLIUI.es.js",
      "require": "./dist/VueGirderSlicerCLIUI.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "publishConfig": {
    "@BryonLewis:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BryonLewis/dive-dsa-slicer/tree/main/vue-girder-slicer-ui"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@jamescoyle/vue-icon": "^0.1.2",
    "@mdi/js": "^7.2.96",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.5.0",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "qs": "^6.11.2",
    "underscore": "^1.13.6",
    "vue": "^3.2.25",
    "vue-collapsed": "^1.2.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-vue": "^2.3.3",
    "bootstrap": "^5.3.1",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "typescript": "^5.2.2",
    "vite": "^2.9.9",
    "vite-plugin-dts": "^3.6.3",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.38.4"
  },
  "//": "vue/no-setup-props-destructure is disabled in esLint, due to an issue with utilizing DefineProps: https://github.com/vuejs/eslint-plugin-vue/issues/2121",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "vue/no-setup-props-destructure": 0,
      "sort-imports": [
        "error",
        {
          "ignoreDeclarationSort": true
        }
      ]
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/vue3-recommended"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended-vue",
      "stylelint-config-html"
    ],
    "rules": {
      "at-rule-no-unknown": [
        true,
        {
          "ignoreAtRules": [
            "tailwind",
            "apply",
            "variants",
            "responsive",
            "screen"
          ]
        }
      ],
      "declaration-block-trailing-semicolon": null,
      "no-descending-specificity": null
    }
  }
}
